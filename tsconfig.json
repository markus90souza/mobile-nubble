{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@infra/*":["./src/infra/*"],
      "@types/*":["./src/types/*"],
      "@assets/*":["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@domain/*":["./src/domain/*"],
      "@hooks/*":["./src/hooks/*"],
      "@routes/*":["./src/routes/*"],
      "@screens/*":["./src/screens/*"],
      "@theme/*":["./src/theme/*"],
      "@brand/*":["./src/brand/*"],
      "@api/*":["./src/api/*"],
      "@libs/*":["./src/libs/*"],
      "@/*": ["./src/*"]
    }

  }
}
